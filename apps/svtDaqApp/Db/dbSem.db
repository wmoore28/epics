  field(SCAN, "Passive") 
  field(DTYP,"Soft Channel")
}

record(longin,SVT:daq:feb:$(FEB):sem_uncorrectable_rd)
{
  field(SCAN, "Passive") 
  field(DTYP,"Soft Channel")
}

record(calc,SVT:feb:0:powerstatus){
	field(SCAN,"Passive")
	field(INPA,"SVT:lv:fe:0:digi:i_rd CPP")
	field(INPB,"SVT:lv:fe:0:digi:i_rd.LOW CPP")
	field(INPC,"SVT:lv:fe:0:digi:i_rd.HIGH CPP")
	field(CALC,"A>B&&A<C")
