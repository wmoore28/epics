record(calc, "$(pref)hps:svt:top:si") {
  field(DESC, "SVT Top Silicon position")
  field(PINI, "YES")
  field(CALC, "A*B+C")
  field(INPA, "$(pref)hps:svt_top:motor.RBV CPP")
#field(INPB, "-0.391")
#field(INPC, "7.472")
  field(INPB, "-0.4509")
  field(INPC, "7.6019")
  #field(INPC, "8.6242")
  field(SCAN, "Passive")
  field(PREC, "3")
  field(EGU , "mm" )
}

