record( ai, "$(pref)hps:svt:top:si_desired" ) {
  field( EGU , "mm" )
  field( SCAN, "Passive" )
  field( DESC, "Desired value for top Si" )
  field( PREC, "3")
  field( SDIS, "$(pref)hps:svt:top:si_desired_sync.PACT" )
  field( FLNK, "$(pref)hps:svt:top:si_calcout PP" )
  field( DISV, "1" )
}

